@model Booking.Models.ViewModel

@{
    ViewData["Title"] = "Book";
    Layout = "_LayoutBack";
}
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">


<div class="container body-content" id="containerPage">
    <form method="POST">
        <div class="main-container">

            <div class="col-lg-9">
                <div class="content-review">
                    <div class="reservation-room-types">
                        <div class="above">
                            <h3><input name="hotel" readonly="readonly" value="@Model.Dados.NomeHotel" id="inputBook"/></h3> 
                            <h4><input name="tipo" readonly="readonly" value="@Model.Dados.TipoQuarto" id="inputBook"/></h4>

                            <div class="row">
                                <div class="col-md-6 col-sm-12">
                                    <label>Nome do Responsável*</label>
                                    <input type="text" name="nomeR" class="form-control" required="required" />
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <label>Sobrenome Responsável*</label>
                                    <input type="text" name="sobrenomeR" class="form-control" required="required" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label>Adultos</label>
                                    <select class="form-control" name="adultos">
                                        <option value="1" label="1">1</option>
                                        <option value="2" selected="selected" label="2">2</option>
                                    </select>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label>Crianças</label>
                                    <select class="form-control" name="criancas">
                                        <option value="0" selected="selected" label="0">0</option>
                                        <option value="1" label="1">1</option>
                                        <option value="2" label="2">2</option>
                                        <option value="3" label="3">3</option>
                                    </select>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label>Quartos</label>
                                    <select class="form-control" name="quant">
                                        <option value="1" selected="selected" label="1">1</option>
                                        <option value="2" label="2">2</option>
                                        <option value="3" label="3">3</option>
                                        <option value="4" label="4">4</option>
                                        <option value="5" label="5">5</option>
                                        <option value="6" label="6">6</option>
                                        <option value="7" label="7">7</option>
                                        <option value="8" label="8">8</option>
                                        <option value="9" label="9">9</option>
                                        <option value="10" label="">10</option>
                                    </select>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label>Regime</label>
                                    <select class="form-control" name="regime">
                                        @foreach (var item in Model.Regimes)
                                        {
                                            <option>@item</option>
                                        }
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="content-review">
                    <h3>Pagamento</h3>
                    <div>
                        <h5>Dados do hóspede</h5>
                        <div class="DadosHosp">
                            <div class="row">
                                <div class="col-md-6">
                                    <label>Nome*</label>
                                    <input type="text" name="nome" class="form-control" required="required" />
                                </div>
                                <div class="col-md-6">
                                    <label>Sobrenome*</label>
                                    <input type="text" name="sobrenome" class="form-control" required="required" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <label>Email</label>
                                    <input type="email" name="email" class="form-control" required="required" />
                                </div>
                                <div class="col-md-6">
                                    <label>Telefone*</label>
                                    <input type="text" name="telefone" class="form-control" required="required" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <label>Endereço*</label>
                                    <input type="text" name="endereco" class="form-control" required="required" />
                                </div>
                                <div class="col-xs-6 col-md-3">
                                    <label>Código Postal*</label>
                                    <input type="text" name="codPostal" class="form-control" required="required" />
                                </div>
                                <div class="col-xs-6 col-md-3">
                                    <label>Localidade*</label>
                                    <input type="text" name="localidade" class="form-control" required="required" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <label>Cartão de Cidadão*</label>
                                    <input type="text" name="cc" class="form-control" required="required" />
                                </div>
                                <div class="col-md-6">
                                    <label>Data de Nascimento*</label>
                                    <input type="text" name="dataNasc" class="form-control" required="required" />
                                </div>
                            </div>
                        </div>
                        <hr />
                        <div style="margin-top:20px">
                            <h5>Dados de Pagamento</h5>
                            <div class="review-payment-details">
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Nome do Titular*</label>
                                        <input type="text" name="nomeTitular" class="form-control" required="required" />
                                    </div>
                                    <div class="col-md-6">
                                        <label>Número do Cartão*</label>
                                        <input type="text" name="numCartao" class="form-control" required="required" />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Tipo do Cartão*</label>
                                        <select class="form-control" name="tipoCartao">
                                            @foreach (var item in Model.TipoPagamento)
                                            {
                                                <option>@item</option>
                                            }
                                        </select>
                                    </div>
                                    <div class="col-md-6" style="margin-top:22px">
                                        <img class="cards-img-valign img-thumbnail" alt="visa-logo.png" src="data:image/png;base64,">
                                        <img class="cards-img-valign img-thumbnail ng-hide" alt="amex-logo.png" src="data:image/png;base64,">
                                        @*<img class="cards-img-valign img-thumbnail ng-hide" alt="discover-logo.png" src="data:image/png;base64,">*@
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Prazo de Validade*</label>
                                        <input type="text" name="prazo" class="form-control" required="required" />
                                    </div>
                                    <div class="col-xs-6 col-md-3">
                                        <label>CVV*</label>
                                        <input type="text" name="cvv" class="form-control" required="required" />
                                    </div>
                                    <div class="col-xs-6 col-md-3" style="margin-top:24px">
                                        <img class="cvv-img-valign" src="data:image/png;base64," alt="cvv-code.png">
                                    </div>
                                </div>

                            </div>
                        </div>

                        <hr />
                    </div>
                    <div>
                        @*<input type="checkbox" name="terms_and_conditions" required="" />
                        <a href="#" terms-and-conditions-link="">Li e aceito os Termos e Condições</a>
                        <br />
                        <input type="checkbox" name="terms_and_conditions" required="" />
                        <span class="ng-binding">Concordo em receber e-mails promocionais da propriedade</span>*@
                        <br />
                        <br />
                        <button asp-action="Book" asp-controller="Home" class="btn btn-success" type="submit" onclick="PopUp()"> Reservar agora <i class="glyphicon glyphicon-chevron-right" style="color:white"></i> </button>
                    </div>
                </div>
            </div>
        </div>

        <div id="right-container" class="col-lg-3">
            <div class="titulo">
                <h4> Descrição da Reserva</h4>
            </div>
            <div class="reserva">
                <div>
                    <strong>Check In:</strong>
                    @*@Html.EditorFor(model => model.Dados.CheckIn)
                        @Html.ValidationMessageFor(model => model.Dados.CheckIn, "", new { @class = "text-danger" })*@
                    <input type="date" class="form-control" value="@Model.Dados.CheckIn.ToString("yyyy-MM-dd")" name="checkIn">
                    <br />
                    <strong>Check Out:</strong>
                    <input type="date" value="@Model.Dados.CheckOut.ToString("yyyy-MM-dd")" class="form-control" name="checkOut">
                </div>
                <hr />
                <div>
                    <strong>@Model.Dados.NomeHotel</strong>
                    <br />
                    <div class="row">
                        <div class="col-xs-8">Capacidade</div>
                        <div class="col-xs-4 pull-right"><input name="capacidade" readonly="readonly" value="@Model.Dados.Capacidade" id="inputBookRight" /></div>
                    </div>
                    <div class="row">
                        <div class="col-xs-8">@Model.Dados.TipoQuarto</div>
                        <div class="col-xs-4 pull-right"><input name="preco" readonly="readonly" value="@Model.Dados.Preco €" id="inputBookRight" /></div>
                    </div>
                    </div>
                </div>
            <div class="titulo">
                <h4> Contacto e Localização</h4>
            </div>
            <div class="hotel">
                <h5>Demo Hotel</h5>

                <div class="row">
                    <div class="col-xs-1">
                        <span class="glyphicon glyphicon-home"></span>
                    </div>
                    <div class="col-xs-10">
                        <span>88 Resort Street, Ecke Musterplatz, Little Cayman, 8000, Austrália</span>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xs-1">
                        <i class="material-icons" style="font-size:15px">directions_car</i>
                    </div>
                    <div class="col-xs-10">
                        <span>
                            Estacionamento grátis,
                            Estacionamento com Segurança,
                            Estacionamento subterrâneo com manobrista (aplica-se taxas),
                            Estacionamento coberto no local,
                            Estacionamento na rua
                        </span>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xs-1">
                        <span class="glyphicon glyphicon-phone-alt"></span>
                    </div>
                    <div class="col-xs-10">
                        <a href="#">1300 123 456</a>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xs-1">
                        <span class="glyphicon glyphicon-phone-alt"></span>
                    </div>
                    <div class="col-xs-10">
                        <a href="tel:61280311222">61280311222</a>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xs-1">
                        <span class="glyphicon glyphicon-envelope"></span>
                    </div>
                    <div class="col-xs-10">
                        <a href="mailto:sales@siteminder.com">sales@siteminder.com</a>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xs-1">
                        <span class="glyphicon glyphicon-globe"></span>
                    </div>
                    <div class="col-xs-10">
                        <a href="http://www.siteminder.com/thebookingbutton">Visitar o sítio Web</a>
                    </div>
                </div>
                <hr />
                <div class="mapa">
                    <a ng-href="https://maps.google.com.au/maps?q=little+cayman&amp;bav=on.2,or.r_cp.r_qf.&amp;bvm=bv.50952593,d.aGc,pv.xjs.s.en_US.epv7Gxs5zsU.O&amp;biw=1680&amp;bih=955&amp;um=1&amp;ie=UTF-8&amp;hl=en&amp;sa=N&amp;tab=wl" target="_blank" href="https://maps.google.com.au/maps?q=little+cayman&amp;bav=on.2,or.r_cp.r_qf.&amp;bvm=bv.50952593,d.aGc,pv.xjs.s.en_US.epv7Gxs5zsU.O&amp;biw=1680&amp;bih=955&amp;um=1&amp;ie=UTF-8&amp;hl=en&amp;sa=N&amp;tab=wl">
                        <img src="data:image/png;base64," alt="thumb-google-maps.png">
                        <span class="details-contact-txt ng-binding">Ver no Google Maps</span>
                    </a>
                </div>
                <hr />
                <p id="pBook"> <button type="button" class="btn btn-default btn-contact">Contacte-nos</button> </p>
            </div>
        </div>
    </form>
</div>
